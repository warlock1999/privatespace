{
  "version": "3",
  "services": {
    "alist": {
      "image": "xhofe/alist:latest",
      "container_name": "alist",
      "restart": "unless-stopped",
      "ports": [
        "5244:5244"
      ],
      "volumes": [
        "./data:/data",
        "./config:/config"
      ],
      "environment": [
        "ADMIN_PASSWORD=${ADMIN_PASSWORD}",
        "PUID=${PUID}",
        "PGID=${PGID}"
      ]
    }
  }
}
