{
  "id": "nginxproxymanager",
  "name": "Nginx Proxy Manager",
  "description": "A reverse proxy management system with a simple web UI, providing free SSL support via Let's Encrypt.",
  "short_desc": "Manage reverse proxies with a user-friendly interface and Let's Encrypt SSL.",
  "version": "latest",
  "tipi_version": "1.0.0", // Changed from number (1) to string to match typical version formats
  "port": 81,
  "categories": ["network", "security"], // Valid categories; no change needed
  "author": "jc21",
  "source": "https://github.com/NginxProxyManager/nginx-proxy-manager",
  "available": true,
  "form_fields": [
    {
      "id": "DB_MYSQL_USER",
      "label": "MySQL User",
      "type": "text",
      "env_variable": "DB_MYSQL_USER",
      "default": "npm"
    },
    {
      "id": "DB_MYSQL_PASSWORD",
      "label": "MySQL Password",
      "type": "password",
      "env_variable": "DB_MYSQL_PASSWORD",
      "default": "npm"
    },
    {
      "id": "DB_MYSQL_NAME",
      "label": "MySQL Database Name",
      "type": "text",
      "env_variable": "DB_MYSQL_NAME",
      "default": "npm"
    },
    {
      "id": "MYSQL_ROOT_PASSWORD",
      "label": "MySQL Root Password",
      "type": "password",
      "env_variable": "MYSQL_ROOT_PASSWORD",
      "default": "npm"
    }
  ],
  "compose_file": "docker-compose.yml" // Changed from "docker-compose.json" to match test expectation
}