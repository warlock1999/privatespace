{
  "schemaVersion": 2,
  "services": [
    {
      "name": "alist",
      "image": "xhofe/alist:latest",
      "restart": "unless-stopped",
      "isMain": true,
      "internalPort": 5244,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/opt/alist/data"
        }
      ],
      "environment": [
        "PUID=1000",
        "PGID=1000",
        "UMASK=022"
      ]
    }
  ]
}