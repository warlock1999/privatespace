{
  "schemaVersion": 2,
  "services": [
    {
      "name": "alist",
      "image": "xhofe/alist:latest",
      "isMain": true,
      "internalPort": "5244",
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "UMASK",
          "value": "022"
        },
        {
          "key": "ADMIN_PASSWORD",
          "value": "changeme"
        },
        {
          "key": "ADMIN_USER",
          "value": "admin"
        }
      ],
      "volumes": [
        {
          "containerPath": "/opt/alist/data",
          "hostPath": "${APP_DATA_DIR}/alist/data",
          "readOnly": false,
          "shared": false,
          "private": true
        }
      ],
      "addPorts": [
        {
          "containerPort": 5244,
          "hostPort": 5244,
          "tcp": true,
          "udp": true
        }
      ]
    }
  ]
}
